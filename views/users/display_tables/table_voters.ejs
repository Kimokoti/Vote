<div class="row">
	<div class="col-lg-4"></div>
	<div class="col-lg-4">
		<div class="label label-primary table_heads">Registered Voters</div>
	</div>
	<div class="col-lg-4"></div>
</div>
<div class="row">
	<div class="col-lg-12">
	<div class="form-group">
		<div class="col-lg-4 col-lg-offset-4"> <input type="text" name="" onkeyup="filter2(this, 'sf', 1)"  placeholder="Enter Search Keyword" class="radius form-control"></div>
	</div>
	<br>
		<table id="sorter" class="table table-striped table-bordered table-responsive table_display" style="border:none" cellspacing="0" width="100%">
				<thead style="background-color:#407A52;font-size:12px">
					<tr>
						<th>Voter ID</th>
						<th>Voter Name</th>
						<th>Gender</th>
						<th>Reg ID</th>
						<th>Vote Status</th>
						<!--<th>Vote Time</th> -->
						<th>Voter Status</th>
						<th colspan="2">Action</th>
					</tr>
				</thead>
				<tbody>
				
				<% voters.forEach( function( voter ) { %>
					<tr>
						<td><%= voter.voter_id %></td>
						<td  style="font-size:14px"><%= voter.votername %></td>
						<td  style="font-size:14px"><%= voter.gender %></td>
						<td  style="font-size:12px"><%= voter.reg_ID %></td>
						<td  style="font-size:14px"><%= voter.vote_status %></td>
						<!--<td><%= voter.vote_time %></td>-->
						<td>
							<% if(voter.voter_status=='yes'){ %>
								<button class="btn btn-success disabled"><i class="fa fa-check-circle-o fa-lg"></i></button>
							<%  }else{ %>
								<button class="btn btn-danger disabled" data-toggle="tooltip" data-placement="left" title="Voter Locked"><i class="fa fa-exclamation-circle fa-lg"></i></button>
							<%  } %>
						</td>
						<td><button class="btn btn-success" data-toggle="tooltip" data-placement="left" title="Edit Voter"><i class="fa fa-edit fa-lg"></i></button></td>
						<td>
							<% if(voter.voter_status=='yes'){ %>
								<button class="btn btn-danger" data-toggle="tooltip" data-placement="left" title="Lock Voter" ><i class="fa fa-lock fa-lg"></i></button>
							<%  }else{ %>
								<button class="btn btn-success" data-toggle="tooltip" data-placement="left" title="Unlock Voter"><i class="fa fa-unlock fa-lg"></i></button>
							<%  } %>
						</td>
					</tr>						
				<% }) %>
				</tbody>
				
		</table>

		<% if (pageCount > 1) { %>
	        <ul class="pagination">
	            <% if (currentPage > 1) { %>
	                <li><a href="/dv?page=1">&laquo;</a></li>
	            <% } %>
	            <% var i = 1;
	            if (currentPage > 5) {
	                i = +currentPage - 4;
	            } %>
	            <% if (i !== 1) { %>
	                <li class="disabled"><a href="#">...</a></li>
	            <% } %>
	            <% for (i; i<=pageCount; i++) { %>
	                <% if (currentPage == i) { %>
	                    <li class="active"><span><%= i %> <span class="sr-only">(current)</span></span></li>
	                <% } else { %>
	                    <li><a href="/dv?page=<%= i %>"><%= i %></a></li>
	                <% } %>
	                <% if (i == (+currentPage + 4)) { %>
	                    <li class="disabled"><a href="#">...</a></li>
	                <% break; } %>
	            <% } %>
	            <% if (currentPage != pageCount) { %>
	                <li><a href="/dv?page=<%= pageCount %>">&raquo;</a></li>
	            <% } %>
	        </ul>
	    <% } %>
	</div>

</div>


