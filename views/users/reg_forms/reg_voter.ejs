<div class="well reg-form">
                    <form class="form-horizontal" method="post" id="reg_voters" action="rv">
                        <fieldset>
                            <legend class="table_heads">Register Voter</legend>

                             <div class="form-group form-reg">
                                    <div class="col-lg-10 col-lg-offset-2 alert alert-success" id="alert-reg"></div>
                            </div>

                            <input type="hidden" id="userType" name="userType" value="voter">

                            <div class="form-group form-reg">
                                    <div class="col-lg-10 col-lg-offset-2 reg_error" id="error-votername"></div>
                            </div>
                           
                            <div class="form-group">
                                <label for="voterName" class="col-lg-2 control-label">Voter Name</label>
                                <div class="col-lg-10">
                                    <input class="form-control" id="voterName" name="voterName" placeholder="Voter Name" type="text">
                                </div>
                            </div>

                            <div class="form-group form-reg">
                                    <div class="col-lg-10 col-lg-offset-2 reg_error" id="error-address"></div>                                   
                            </div>

                            <div class="form-group">
                                <label for="inputEmail" class="col-lg-2 control-label">Email</label>
                                <div class="col-lg-10">
                                    <input class="form-control" id="inputEmail" name="inputEmail" placeholder="Email" type="text">
                                </div>
                            </div>

                            <div class="form-group form-reg">
                                    <div class="col-lg-10 col-lg-offset-2 reg_error" id="error-reg"></div>
                            </div>

                            <div class="form-group">
                                <label for="regNumber"  class="col-lg-2 control-label">Reg Number</label>
                                <div class="col-lg-10">
                                    <input class="form-control" id="regNumber" name="regNumber" placeholder="Registration Number" type="text">
                                </div>
                            </div>

                            <div class="form-group form-reg">
                                    <div class="col-lg-10 col-lg-offset-2 reg_error" id="error-gender"></div>
                            </div>

                             <div class="form-group">
                                <label for="rad_gender" class="col-lg-2 control-label">Gender</label>
                                <div class="col-lg-10">      
                                        <label class="radio-inline">
                                            <input type="radio" name="rad_gender" id="gender1" value="Female" >Female
                                        </label>
                                        <label class="radio-inline">
                                            <input type="radio" name="rad_gender" id="gender2" value="Male" >Male
                                        </label>
                                </div>
                            </div>

                            <div class="form-group form-reg">
                                    <div class="col-lg-10 col-lg-offset-2 reg_error" id="error-position">                 
                                    </div>
                            </div>

                             <div class="form-group form-reg">
                                    <div class="col-lg-10 col-lg-offset-2 reg_error" id="error-res"></div>
                            </div>


                            <div class="form-group">
                                <label for="rad_res" class="col-lg-2 control-label">Residence</label>
                                <div class="col-lg-10">      
                                        <label class="radio-inline">
                                            <input type="radio" name="rad_res" id="res1" value="Inside">Inside Campus
                                        </label>
                                        <label class="radio-inline">
                                            <input type="radio" name="rad_res" id="res2" value="Outside">Outside Campus
                                        </label>
                                </div>
                            </div>
                           
                            <div class="form-group form-reg">
                                    <div class="col-lg-10 col-lg-offset-2 reg_error" id="error-pass"></div>
                            </div>

                            <div class="form-group">
                                <label for="password" class="col-lg-2 control-label">Password</label>
                                <div class="col-lg-10">
                                    <input class="form-control" id="inputPassword" name="password" placeholder="Password" type="password">
                                </div>
                            </div>

                            <div class="form-group form-reg">
                                    <div class="col-lg-10 col-lg-offset-2 reg_error" id="error-confpass"></div>
                            </div>

                            <div class="form-group">
                                <label for="inputPasswordConf" class="col-lg-2 control-label">Confirm Password</label>
                                <div class="col-lg-10">
                                    <input class="form-control" id="inputPasswordConf" placeholder="Confirm Password" name="conf_password" type="password">
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-lg-4 col-lg-offset-8">
                                    <input type="submit" name="reg" id="register" value="REGISTER VOTER" class="btn btn-success">
                                </div>
                            </div>
                        </fieldset>
                    </form>
</div>
                